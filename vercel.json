{
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/server.js",
        "dest": "/api/server.js"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
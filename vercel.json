{
    "builds": [
      {
        "src": "api/hello.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/hello",
        "dest": "/api/hello.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }